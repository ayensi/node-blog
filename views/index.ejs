<div class="container">
    <% data.articles.forEach(article=>{ %>

<div style="align-items: center; float: left; margin-right: 50px; margin-top: 50px;" >
    <div class="card border-dark mb-3" style="max-width: 35rem; width: 30rem;">
        <div class="card-header"> <%= article.category%></div>
        <div class="card-body">
          <h4 class="card-title"><%= article.title%></h4>
          <p class="card-text"><%= article.body%></p>
        </div>
        <button style="max-width: 10rem; align-items: center;" type="button" class="btn btn-outline-light">Read More</button>
      </div>
</div>
<% }) %>
</div>

